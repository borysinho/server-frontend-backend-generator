    {
      "name": "Code Generation",
      "item": [
        {
          "name": "Generate Backend (Spring Boot)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"diagramState\": {\n    \"elements\": {\n      \"class1\": {\n        \"id\": \"class1\",\n        \"type\": \"class\",\n        \"name\": \"Usuario\",\n        \"attributes\": [\n          {\"name\": \"id\", \"type\": \"Long\", \"visibility\": \"private\"},\n          {\"name\": \"nombre\", \"type\": \"String\", \"visibility\": \"private\"},\n          {\"name\": \"email\", \"type\": \"String\", \"visibility\": \"private\"}\n        ],\n        \"methods\": [\n          {\"name\": \"getNombre\", \"returnType\": \"String\", \"visibility\": \"public\"}\n        ]\n      }\n    },\n    \"relationships\": {}\n  },\n  \"diagramName\": \"Sistema de Usuarios\",\n  \"databaseConfig\": {\n    \"type\": \"postgresql\",\n    \"host\": \"localhost\",\n    \"port\": 5432,\n    \"database\": \"mi_base_datos\",\n    \"username\": \"usuario\",\n    \"password\": \"password\"\n  },\n  \"diagramId\": \"{{diagram_id}}\",\n  \"creatorId\": \"{{user_id}}\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/diagrams/generate-backend",
              "host": ["{{base_url}}"],
              "path": ["api", "diagrams", "generate-backend"]
            },
            "description": "Generar código backend Spring Boot a partir de un diagrama UML"
          }
        },
